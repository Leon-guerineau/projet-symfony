<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Projet Symfony{% endblock %}</title>
    <link rel="stylesheet" href="https://bootswatch.com/5/darkly/bootstrap.min.css">
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
{% apply inline_css(source("style.css")) %}
    <body>
        {% include 'navbar.html.twig' %}
        {% include 'admin-navbar.html.twig' %}
        {% include '_flashes.html.twig' %}
        <div class="header">{% block header %}{% endblock %}</div>
        <div class="main">{% block body %}{% endblock %}</div>
    </body>
{% endapply %}
</html>
